{
    "name": "Warm Light",
    "description": "Applies a warm color, reducing blue light and eye strain",
    "version": "1.0",
    "manifest_version": 3,
    "permissions": ["activeTab", "scripting"],
    "background": {
    "service_worker": "background.js"
    },
    "action": {
      "default_popup": "ext.html",
      "default_icon": "icon_nobkg.png"
    },
    "content_scripts": [
    {
      "matches": ["<all_urls>"],
      "js": ["content.js"]
    }
  ]
  }